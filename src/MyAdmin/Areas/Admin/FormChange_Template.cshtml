@using Microsoft.AspNetCore.Html;
@using MyAdmin.Admin;

@{
    Layout = "~/Areas/Admin/_Layout.cshtml";
    IRenderable formAdd = (ViewData["Form"] as IRenderable)!;
}

<div class="container">
    <h1 class="display-4">@ViewData["ModelName"] @ViewData["identifier"]</h1>

    <p>Editing @ViewData["instance"]!.ToString()</p>

    @(new HtmlString(formAdd.Render()))
</div>
